{
  "manifest_version": 3,

  "name": "regexp-search",
  "description": "regexp-search",
  "version": "1.0.0",

  "commands": {
    "Ctrl+F": {
      "suggested_key": {
        "default": "Ctrl+F",
        "mac": "Command+F"
      },
      "description": "Run regular expression search on the current page."
    }
  },
  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["activeTab", "background", "scripting"],
  "host_permissions": ["<all_urls>"]
}
